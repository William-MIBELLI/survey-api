{
  "name": "survey-api",
  "version": "1.0.0",
  "description": "API for SurveyGen",
  "scripts": {
    "dev": "nodemon -L src/index.ts",
    "start": "NODE_PATH=./build node --env-file=.env build/src/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "codegen": "graphql-codegen --config codegen.ts",
    "seed": "docker container exec survey-gen-backend-1 npx ts-node -r tsconfig-paths/register src/utils/seed.utils.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/William-MIBELLI/survey-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "imports": {
    "#*": "./src/*"
  },
  "bugs": {
    "url": "https://github.com/William-MIBELLI/survey-api/issues"
  },
  "homepage": "https://github.com/William-MIBELLI/survey-api#readme",
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.1.1",
    "@graphql-tools/schema": "^10.0.25",
    "argon2": "^0.44.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-constraint-directive": "^6.0.0",
    "graphql-scalars": "^1.24.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/typescript": "5.0.2",
    "@graphql-codegen/typescript-resolvers": "5.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
